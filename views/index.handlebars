<div class="d-none" id="hbsObj">{{#each cup}},,&#123;"x":{{players}},"y":{{cutPoints}}&#125;{{/each}}</div>
<div class="container my-5 bg-white opaque">
    <div class="row">
        <h1>Meta Matchup Poll Data</h1>
    </div>
    <div class="row">
        <h1>Cup Cut Data</h1>
    </div>
    <div class="row pl-3">
        <h2>Chart</h2>
    </div>
    <div class="row">
        <canvas id="scatterChart"></canvas>
    </div>
    <div class="row pl-3">
        <h2>Table</h2>
    </div>
    <div class="row pl-5 pr-2">
        <div class="table-responsive">
            <table id="dt-select" class="table table-striped table-bordered" cellspacing="0" width="100%">
                <thead>
                    <tr>
                        <th>Players in Division</th>
                        <th>Players Grouped in</th>
                        <th>Rounds</th>
                        <th>Lowest Points in Cut</th>
                        <th>Cut Size</th>
                        <th>Bubbled In</th>
                        <th>Bubbled Out</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each cup}}
                    <tr role="row">
                        <th>{{players}}</th>
                        <th>{{players2}}</th>
                        <th>{{rounds}}</th>
                        <th>{{cutPoints}}</th>
                        <th>{{cutSize}}</th>
                        <th>{{bubbledIn}}</th>
                        <th>{{bubbledOut}}</th>
                    </tr>
                    {{/each}}
                </tbody>
                <tfoot>
                    <tr>
                        <th>Players</th>
                        <th>Players Grouped in</th>
                        <th>Rounds</th>
                        <th>Lowest Points in Cut</th>
                        <th>Cut Size</th>
                        <th>Bubbled In</th>
                        <th>Bubbled Out</th>
                    </tr>
                </tfoot>
            </table>
        </div>
    </div>
    <div class="row">
        <h1>Standard Matchups Pole Results</h1>
        <div id="matchupsObject" class="d-none">
            {{#each matchup}}&#123;"deck1":"{{deck1}}","deck2":"{{deck2}}","wins":"{{wins}}","losses":"{{losses}}","ties":"{{ties}}"&#125;-,{{/each}}
        </div>
        <div id="decksObject" class="d-none">
            {{#each deck}}&#123;"id":"{{id}}","deck":"{{deck}}"&#125;-,{{/each}}
        </div>
    </div>
    <div class="row pl-3">
        <div class="col-5 col-sm-4 col-md-2">
            <ul class="list-group">
                {{#each deck}}
                <li class="list-group-item matchup-tab" data-target="deck-graph-{{id}}">{{deck}}</li>
                {{/each}}
            </ul>
        </div>
        <div class="col-7 col-sm-8 col-md-10 d-flex flex-column justify-content-center" id="graphs">
            {{#each deck}}
            <div class="d-none deck-graph" id="deck-graph-{{id}}">
            </div>
            {{/each}}
        </div>
    </div>
    <div class="row">
        <h1>Expanded Matchups Pole Results</h1>
        <div id="matchupsExpObject" class="d-none">
            {{#each matchupExp}}&#123;"deck1":"{{deck1}}","deck2":"{{deck2}}","wins":"{{wins}}","losses":"{{losses}}","ties":"{{ties}}"&#125;-,{{/each}}
        </div>
        <div id="decksExpObject" class="d-none">
            {{#each deckExp}}&#123;"id":"{{id}}","deck":"{{deck}}"&#125;-,{{/each}}
        </div>
    </div>
    <div class="row pl-3">
        <div class="col-5 col-sm-4 col-md-2">
            <ul class="list-group">
                {{#each deckExp}}
                <li class="list-group-item matchup-exp-tab" data-target="deck-exp-graph-{{id}}">{{deck}}</li>
                {{/each}}
            </ul>
        </div>
        <div class="col-7 col-sm-8 col-md-10 d-flex flex-column justify-content-center" id="graphs-exp">
            {{#each deckExp}}
            <div class="d-none deck-exp-graph" id="deck-exp-graph-{{id}}">
            </div>
            {{/each}}
        </div>
    </div>
</div>
</div>